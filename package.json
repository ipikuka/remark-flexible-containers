{
  "name": "remark-flexible-containers",
  "version": "1.0.0",
  "description": "Remark plugin to add custom containers with customizable properties in markdown",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts}\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.cjs",
    "test:file": "NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.cjs with_options.spec.ts",
    "prepack": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipikuka/remark-flexible-containers.git"
  },
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "markdown",
    "remark-containers",
    "remark-flexible-containers",
    "remark-custom-containers"
  ],
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ipikuka/remark-flexible-containers#readme",
  "bugs": {
    "url": "https://github.com/ipikuka/remark-flexible-containers/issues"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/unist": "^2.0.6",
    "@types/vfile": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "dedent": "^0.7.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "~4.9.4",
    "unified": "^10.1.2"
  },
  "dependencies": {
    "@types/mdast": "^3.0.10",
    "unist-util-find": "^1.0.2",
    "unist-util-find-after": "^4.0.1",
    "unist-util-find-all-between": "^2.1.0",
    "unist-util-visit": "^4.0.0"
  },
  "sideEffects": false
}
